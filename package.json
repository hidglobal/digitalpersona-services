{
  "name": "access-management.js",
  "version": "0.0.1",
  "description": "JavaScript Client for DigitalPersona Web Access Management",
  "author": "Andrey Bronnikov",
  "license": "(c) HID Global 2019. All rights reserved.",
  "keywords": [
    "DigitalPersona",
    "authentication",
    "identification",
    "jwt",
    "policy",
    "claims",
    "enroll",
    "claims",
    "secret",
    "ActiveDirectory",
    "LSD",
    "Altus"
  ],
  "module": "./dist/es6/index.js",
  "main": "./dist/commonjs/index.js",
  "typings": "./dist/commonjs/index.d.ts",
  "scripts": {
    "rollup-min": "rollup -c rollup.config.js --environment minify:true",
    "rollup": "rollup -c rollup.config.js",
    "tsc": "tsc",
    "tsc-es6": "tsc --p tsconfig.es6.json",
    "build": "rimraf ./dist && npm run rollup && npm run rollup-min && npm run tsc && npm run tsc-es6",
    "test": "karma start",
    "test-browser": "karma start --single-run",
    "test-node": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine-node.js",
    "test-all": "npm run test-browser && npm run test-node",
    "test-common": "set UUT=common&& karma start",
    "test-auth": "set UUT=auth&& karma start",
    "test-policy": "set UUT=policy&& karma start",
    "test-secrets": "set UUT=secrets&& karma start",
    "test-claims": "set UUT=claims&& karma start",
    "test-admin": "set UUT=admin&& karma start",
    "test-enroll": "set UUT=enroll&& karma start",
    "test-exclude-uncompressed": "set EXCLUDE_UNCOMPRESSED=true&& karma start",
    "test-node-exclude-uncompressed": "set EXCLUDE_UNCOMPRESSED=true&& ts-node node_modules/jasmine/bin/jasmine --config=jasmine-node.js",
    "docs": "typedoc --options ./typedoc.config.js ./src"
  },
  "dependencies": {
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.12.12",
    "jasmine": "^3.3.0",
    "jasmine-core": "^3.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-typescript": "^3.0.12",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.3",
    "rollup": "^0.65.0",
    "rollup-plugin-commonjs": "^9.1.6",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "3.3.3"
  }
}
